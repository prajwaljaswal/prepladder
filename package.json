  "lint-staged": {
    "*.js": "eslint --fix ",
    "*.{js,css,md,html,json}": "prettier --cache --write"
},
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
